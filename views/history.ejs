<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Withdrawal History</title>
    <link href="./history.css" rel="stylesheet">
    <link rel="shortcut icon" href="/logo.svg" type="image/x-icon">

    <style>
        .status-approved {
            background-color: #D1FAE5;
            color: #065F46;
        }

        .status-declined {
            background-color: #FEE2E2;
            color: #991B1B;
        }

        .status-pending {
            background-color: #FEF3C7;
            color: #92400E;
        }
    </style>
</head>

<body class="bg-gray-50 pt-16 pb-20">
    <!-- Header -->
    <header class="fixed w-full top-0 z-50 bg-white shadow-md">
        <nav class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="flex justify-between items-center h-16">
                <!-- Logo -->
                <div class="flex-shrink-0">
                    <a href="#" class="flex items-center">
                        <img class="h-16 w-auto" src="logo.svg" alt="Logo">
                        <span class="ml-2 text-xl font-bold text-blue-800">FOURx</span>
                    </a>
                </div>

                <!-- Desktop Navigation -->
                <div class="hidden md:flex items-center space-x-8">
                    <a href="" id="homebtn11" class="text-gray-600 hover:text-blue-600 transition duration-300">Home</a>
                    <a href="" id="assetsbtn11"
                        class="text-gray-600 hover:text-blue-600 transition duration-300">Assets</a>
                    <a href="" id="aboutbtn333"
                        class="text-gray-600 hover:text-blue-600 transition duration-300">about</a>

                </div>

                <!-- Mobile Menu Button -->
                <!-- Mobile Menu (add this) -->
                <div id="mobile-menu" class="hidden md:hidden bg-white shadow-lg">
                    <div class="px-2 pt-2 pb-3 space-y-1">
                        <a href="#" id="mobile-home" class="block px-3 py-2 text-gray-600 hover:text-blue-600">Home</a>
                        <a href="#" id="mobile-assets"
                            class="block px-3 py-2 text-gray-600 hover:text-blue-600">Assets</a>
                        <a href="#" id="aboutbtn33" class="block px-3 py-2 text-gray-600 hover:text-blue-600">about</a>
                    </div>
                </div>
            </div>
        </nav>
    </header>

    <!-- Main Content -->
    <main class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
        <div class="bg-white shadow rounded-lg overflow-hidden">
            <!-- Page Header -->
            <div class="px-6 py-4 border-b border-gray-200">
                <h1 class="text-2xl font-bold text-gray-800">Withdrawal History</h1>
            </div>

            <!-- Simplified Withdrawal Table -->
            <div class="overflow-x-auto">
                <table class="min-w-full divide-y divide-gray-200">
                    <thead class="bg-gray-50">
                        <tr>
                            <th scope="col"
                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Date</th>
                            <th scope="col"
                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Amount</th>
                            <th scope="col"
                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Payment Method</th>
                            <th scope="col"
                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                Status</th>
                        </tr>
                    </thead>
                    <tbody class="bg-white divide-y divide-gray-200">
                        <!-- Approved Withdrawal -->
                        <tr class="hover:bg-gray-50">
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">May 15, 2023</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">$1,250.00</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">Bank Transfer</td>
                            <td class="px-6 py-4 whitespace-nowrap">
                                <span class="px-3 py-1 text-xs font-medium rounded-full status-approved">Approved</span>
                            </td>
                        </tr>

                        <!-- Declined Withdrawal -->
                        <tr class="hover:bg-gray-50">
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">Apr 28, 2023</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">$750.00</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">PayPal</td>
                            <td class="px-6 py-4 whitespace-nowrap">
                                <span class="px-3 py-1 text-xs font-medium rounded-full status-declined">Declined</span>
                            </td>
                        </tr>

                        <!-- Pending Withdrawal -->
                        <tr class="hover:bg-gray-50">
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">Jun 2, 2023</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">$2,000.00</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">Crypto (BTC)</td>
                            <td class="px-6 py-4 whitespace-nowrap">
                                <span class="px-3 py-1 text-xs font-medium rounded-full status-pending">Pending</span>
                            </td>
                        </tr>

                        <!-- More entries -->
                        <tr class="hover:bg-gray-50">
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">Mar 10, 2023</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">$500.00</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">Bank Transfer</td>
                            <td class="px-6 py-4 whitespace-nowrap">
                                <span class="px-3 py-1 text-xs font-medium rounded-full status-approved">Approved</span>
                            </td>
                        </tr>

                        <tr class="hover:bg-gray-50">
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">Feb 18, 2023</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">$1,000.00</td>
                            <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">PayPal</td>
                            <td class="px-6 py-4 whitespace-nowrap">
                                <span class="px-3 py-1 text-xs font-medium rounded-full status-declined">Declined</span>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </main>

    <!-- Footer Navigation with SVG icons -->
    <nav class="fixed bottom-0 w-full bg-white shadow-[0_-2px_10px_rgba(0,0,0,0.1)] flex justify-around py-3 z-50">
        <a href="" class="flex flex-col items-center text-blue-700 px-2">
            <svg class="h-6 w-6 text-blue-700" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                    d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6" />
            </svg>
            <span id="homebtn22" class="text-xs md:text-sm">Home</span>
        </a>
        <a href="" class="flex flex-col items-center text-gray-500 hover:text-blue-600 px-2">
            <svg class="h-6 w-6 text-blue-700" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                    d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z" />
            </svg>
            <span id="assetsbtn22" class="text-xs md:text-sm">Assets</span>
        </a>
        <a href="" class="flex flex-col items-center text-gray-500 hover:text-blue-600 px-2">
            <svg class="h-6 w-6 text-blue-700" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                    d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z" />
            </svg>
            <span id="teambtn22" class="text-xs md:text-sm">Team</span>
        </a>
        <a href=""
            class="flex flex-col items-center text-gray-500 hover:text-blue-600 px-2 transition-colors duration-200 group"
            aria-label="User profile">
            <svg class="h-6 w-6 text-blue-700 group-hover:text-blue-600" fill="none" viewBox="0 0 24 24"
                stroke="currentColor" aria-hidden="true">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                    d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />
            </svg>
            <span id="profilebtn22" class="text-xs md:text-sm group-hover:text-blue-600">Profile</span>
        </a>
    </nav>

    <script>


        document.getElementById("aboutbtn333").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/about'; // Redirect
        });
        document.getElementById("aboutbtn33").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/about'; // Redirect
        });
        document.getElementById("teambtn22").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/team'; // Redirect
        });
        document.getElementById("assetsbtn11").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/assets'; // Redirect
        });
        document.getElementById("assetsbtn22").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/assets'; // Redirect
        });
        document.getElementById("profilebtn22").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/profile'; // Redirect
        });
        document.getElementById("homebtn11").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/home'; // Redirect
        });
        document.getElementById("homebtn22").addEventListener("click", function (e) {
            e.preventDefault(); // Prevent default navigation
            window.location.href = '/home'; // Redirect
        });
        // Mobile Menu Toggle
        // Mobile Menu Toggle
        function toggleMenu() {
            const mobileMenu = document.getElementById('mobile-menu');
            const menuBtn = document.getElementById('menu-btn');

            if (mobileMenu && menuBtn) {
                mobileMenu.classList.toggle('hidden');
                menuBtn.classList.toggle('text-blue-600');
            }
        }

        // Attach event listener (add this)
        document.addEventListener('DOMContentLoaded', function () {
            const menuBtn = document.getElementById('menu-btn');
            if (menuBtn) {
                menuBtn.addEventListener('click', function (e) {
                    e.preventDefault();
                    toggleMenu();
                });
            }

            // Your existing navigation handlers...
        });
    </script>
</body>

</html>